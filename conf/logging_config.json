{
    "formatters": {
      "json": {
        "class": "pythonjsonlogger.jsonlogger.JsonFormatter"
      }
    },
    "handlers": {
      "console": {
        "class" : "logging.StreamHandler",
        "formatter": "json",
        "level": "INFO",
        "stream": "ext://sys.stdout"
      }
    },
    "loggers": {
      "vllm": {
        "handlers": ["console"],
        "level": "CRITICAL",
        "propagate": false
      }
    },
    "version": 1
  }